{
  "name": "bees",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:server & npm run start:client",
    "start:client": "npm run build:client:dev && npm run wds",
    "start:server": "nodemon --exec babel-node src/server",
    "precommit": "npm run lint:js",
    "prepush": "npm run lint:js",
    "test:prod": "npm run build:client:prod && npm run wds",
    "build:client:dev": "npx cross-env NODE_ENV=dev && npx webpack",
    "build:client:prod": "npx rimraf dist && npx cross-env NODE_ENV=production && npx webpack",
    "wds": "npx webpack-dev-server --content-base dist/ --host 0.0.0.0 --watch-content-base",
    "deploy": "npm run build:client:prod && git add . && git commit -m ':rocket: deploy' && git push && git subtree push --prefix dist origin gh-pages"
  },
  "author": "Naomi Hauret",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "css-hot-loader": "^1.3.2",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.12.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "socket.io": "^2.0.3",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "gsap": "^1.20.3",
    "mongoose": "^4.12.1",
    "socket.io-client": "^2.0.3",
    "three": "^0.87.1"
  }
}
